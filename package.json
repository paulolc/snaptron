{
  "name": "snaptron",
  "version": "0.0.1",
  "description": "It's snap, the visual, blocks based programming language stuffed in electron to control mbots",
  "main": "startup.js",
  "dependencies": {
    "electron-prebuilt": "^1.1.0",
    "johnny-five": "^0.9.19",
    "node-localstorage": "^1.1.2",
    "serialport": "^3.1.2"
  },
  "devDependencies": {
    "node-gyp": "^3.3.1",
    "node-pre-gyp": "^0.6.28"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build --runtime=electron --target=1.1.0 --target_arch=x64 --directory=node_modules/serialport/ --update-binary --dist-url=https://atom.io/download/atom-shell",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulolc/snaptron.git"
  },
  "keywords": [
    "snap",
    "nodebots",
    "blocks",
    "visual",
    "programming",
    "language"
  ],
  "author": "plcarvalho@gmail.com",
  "license": "ISC",
  "build": {}
}
